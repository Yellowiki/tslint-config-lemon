{
  "defaultSeverity": "error",
  "extends": [
    "tslint-plugin-prettier",
    "tslint-config-airbnb",
    "tslint:recommended",
    "tslint:all",
    "tslint-plugin-blank-line",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "blank-line": true,
    "completed-docs": false,
    "no-empty": false,
    "no-any": true
  }
}
